{
  "name": "ember-aria-switch",
  "version": "1.0.11",
  "description": "ARIA Switch button in an Ember addon.",
  "keywords": [
    "ember-addon",
    "aria-switch",
    "toggle",
    "toggle-switch",
    "toggle-button",
    "accessibility",
    "a11y"
  ],
  "license": "MIT",
  "author": "Rajasegar Chandran",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/rajasegar/ember-aria-switch",
  "scripts": {
    "build": "ember build",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember server",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "COVERAGE=true ember test",
    "test:ember-compatibility": "ember try:each",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "ember-cli-babel": "^7.4.1",
    "ember-cli-htmlbars": "^5.0.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "coveralls": "^3.1.0",
    "ember-addon-starterkit": "^0.1.11",
    "ember-ajax": "^5.0.0",
    "ember-cli": "~3.19.0",
    "ember-cli-code-coverage": "github:kategengler/ember-cli-code-coverage#v1.0.0-beta.9",
    "ember-cli-dependency-checker": "^3.1.0",
    "ember-cli-eslint": "^5.1.0",
    "ember-cli-github-pages": "^0.2.1",
    "ember-cli-htmlbars-inline-precompile": "^3.0.0",
    "ember-cli-inject-live-reload": "^2.0.1",
    "ember-cli-qunit": "^4.4.0",
    "ember-cli-release": "^1.0.0-beta.2",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-cli-yuidoc": "^0.9.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^2.0.0",
    "ember-maybe-import-regenerator-for-testing": "^1.0.0",
    "ember-resolver": "^8.0.0",
    "ember-source": "~3.18.0",
    "ember-source-channel-url": "^2.0.1",
    "ember-template-lint": "^2.8.0",
    "ember-try": "^1.4.0",
    "ember-welcome-page": "^4.0.0",
    "eslint": "^7.1.0",
    "eslint-plugin-disable-features": "^0.1.3",
    "eslint-plugin-ember": "^8.5.2",
    "eslint-plugin-node": "^11.1.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "semantic-release": "^17.0.8",
    "yuidoc-ember-theme": "^2.0.1"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://rajasegar.github.io/ember-aria-switch"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "CHANGELOG.md"
          ],
          "message": "release(version): Release ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  }
}
