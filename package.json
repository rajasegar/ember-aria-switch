{
  "name": "ember-aria-switch",
  "version": "2.1.0",
  "description": "ARIA Switch button in an Ember addon.",
  "keywords": [
    "ember-addon",
    "aria-switch",
    "toggle",
    "toggle-switch",
    "toggle-button",
    "accessibility",
    "a11y"
  ],
  "license": "MIT",
  "author": "Rajasegar Chandran",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/rajasegar/ember-aria-switch",
  "scripts": {
    "build": "ember build",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember server",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "COVERAGE=true ember test",
    "test:ember-compatibility": "ember try:each",
    "test:try-one": "ember try:one",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "ember-cli-babel": "^7.19.0",
    "ember-cli-htmlbars": "^4.3.1"
  },
  "devDependencies": {
    "@ember/optional-features": "^1.3.0",
    "@glimmer/component": "^1.0.1",
    "@glimmer/tracking": "^1.0.1",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "coveralls": "^3.1.0",
    "ember-auto-import": "^1.5.3",
    "ember-cli": "~3.22.0",
    "ember-cli-code-coverage": "^1.0.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-github-pages": "^0.2.1",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.0",
    "ember-source": "~3.23.0",
    "ember-source-channel-url": "^2.0.1",
    "ember-template-lint": "^2.6.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.9.0",
    "eslint-plugin-ember": "^10.1.1",
    "eslint-plugin-node": "^11.1.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "semantic-release": "^17.0.8",
    "qunit-dom": "^1.2.0"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://rajasegar.github.io/ember-aria-switch"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "CHANGELOG.md"
          ],
          "message": "release(version): Release ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  }
}
